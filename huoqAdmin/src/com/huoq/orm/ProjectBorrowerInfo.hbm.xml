<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.huoq.orm.ProjectBorrowerInfo" table="project_borrower_info" >
        <id name="id" type="java.lang.Integer">
            <column name="id"  />
            <generator class="identity" />
        </id>
        <property name="productId" type="java.lang.String" column="productId" />
        <property name="productTitle" type="java.lang.String" column="productTitle" />
        <property name="productCreate" type="java.sql.Timestamp" column="productCreate" />
        <property name="borrowerInfoId" type="java.lang.Integer" column="borrowerInfoId" />
        <property name="boUserName" type="java.lang.String" column="boUserName" />
        <property name="boMobile" type="java.lang.String" column="boMobile" />
        <property name="boAmount" type="big_decimal" column="boAmount" />
        <property name="termLoan" type="java.lang.Integer" column="termLoan" />
        <property name="boCreate" type="java.sql.Timestamp" column="boCreate" />
        <property name="raiseFunds" type="big_decimal" column="raiseFunds" />
        <property name="boContractNo" type="java.lang.String" column="boContractNo" />
        <property name="boContractUrl" type="java.lang.String" column="boContractUrl" />
    </class>
</hibernate-mapping>


