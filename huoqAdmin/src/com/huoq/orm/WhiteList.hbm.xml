<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.huoq.orm.WhiteList" table="white_list" >
        <id name="id" type="java.lang.Integer">
            <column name="id"  />
        </id>
        <property name="source" type="java.lang.String" column="source" />
        <property name="iType" type="java.lang.Integer" column="iType" />
        <property name="values" type="java.lang.String" column="values" />
        <property name="status" type="java.lang.Integer" column="status" />
        <property name="dtCreate" type="java.util.Date" column="dtCreate" />
        <property name="dtModify" type="java.util.Date" column="dtModify" />
    </class>
</hibernate-mapping>


